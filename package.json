{
  "name": "online-edu",
  "version": "1.0.0",
  "description": "Electron application for online education",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "trongnd.onlineedu",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "requestedExecutionLevel": "requireAdministrator",
      "target": [
        "portable"
      ]
    },
    "portable": {
      "artifactName": "online_edu_portable.exe"
    },
    "extraResources": [
      {
        "from": "./function/win32",
        "to": "function/win32",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "repository": "",
  "keywords": [],
  "author": "trongnd",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^13.1.6",
    "electron-builder": "^22.11.7"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "node-fetch": "^2.6.1"
  }
}
