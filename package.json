{
  "name": "online-edu",
  "version": "1.0.0",
  "description": "Electron application for online education",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "trongnd.onlineedu",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "requestedExecutionLevel": "requireAdministrator",
      "target": [
        "portable"
      ]
    },
    "portable": {
      "artifactName": "online_edu_portable.exe"
    },
    "directories": {
      "output": "electron/output",
      "app": "electron/app",
      "buildResources": "electron/buildResources"
    }
  },
  "repository": "",
  "keywords": [],
  "author": "trongnd",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^13.1.6"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "electron-builder": "^22.11.7",
    "node-fetch": "^2.6.1"
  }
}
